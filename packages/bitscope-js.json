mber\n    // Careful, the buffers are reused underneath.\n    var name;\n    for (name in buffers)\n      console.log(buffers[name]);\n\n    setTimeout(captureLoop, 500);\n  });\n}\n\ncaptureLoop();\n```\n","starsCount":1,"created":"2016-01-21T06:12:43.428Z","modified":"2016-09-08T09:01:37.671Z","lastPublisher":{"name":"prollings","email":"patrickrollings@gmail.com"},"owners":[{"name":"prollings","email":"patrickrollings@gmail.com"}],"other":{"_attachments":{},"_from":".","_id":"bitscope-js","_nodeVersion":"5.12.0","_npmOperationalInternal":{"host":"packages-12-west.internal.npmjs.com","tmp":"tmp/bitscope-js-1.3.2.tgz_1473325297442_0.292438613018021"},"_npmUser":{"name":"prollings","email":"patrickrollings@gmail.com"},"_npmVersion":"3.8.6","_rev":"1-8ffd2586c9aebc3eee76832d245f26dc","_shasum":"836151d01ca9b20f16008fda564a7c4a27848e31","author":{"name":"Patrick Rollings"},"directories":{},"dist-tags":{"latest":"1.3.2"},"dist":{"shasum":"836151d01ca9b20f16008fda564a7c4a27848e31","tarball":"http://registry.npmjs.org/bitscope-js/-/bitscope-js-1.3.2.tgz"},"maintainers":[{"name":"prollings","email":"patrickrollings@gmail.com"}],"readmeFilename":"README.md","time":{"modified":"2016-09-08T09:01:37.671Z","created":"2016-01-21T06:12:43.428Z","1.0.0":"2016-01-21T06:12:43.428Z","1.0.1":"2016-01-21T06:22:52.546Z","0.0.1":"2016-01-21T06:26:57.925Z","1.0.2":"2016-01-21T06:27:55.633Z","1.0.4":"2016-01-21T06:45:43.157Z","1.0.5":"2016-01-21T06:54:24.173Z","1.0.6":"2016-01-21T06:55:07.126Z","1.0.7":"2016-01-21T06:59:38.217Z","1.0.8":"2016-01-21T07:01:38.227Z","1.0.9":"2016-01-22T02:54:26.709Z","1.1.0":"2016-01-22T03:21:43.501Z","1.1.1":"2016-01-22T03:24:36.673Z","1.1.4":"2016-02-09T05:14:19.587Z","1.1.5":"2016-02-09T05:15:21.790Z","1.2.0":"2016-02-23T04:20:52.275Z","1.3.0":"2016-03-30T05:58:44.319Z","1.3.1":"2016-03-30T06:05:58.671Z","1.3.2":"2016-09-08T09:01:37.671Z"},"users":{"prollings":true}}}
\r\nvar Kinect2 = require('kinect2');\r\n\r\nvar kinect = new Kinect2();\r\n\r\nif(kinect.open()) {\r\n\tconsole.log(\"Kinect Opened\");\r\n\t//listen for body frames\r\n\tkinect.on('bodyFrame', function(bodyFrame){\r\n\t\tfor(var i = 0;  i < bodyFrame.bodies.length; i++) {\r\n\t\t\tif(bodyFrame.bodies[i].tracked) {\r\n\t\t\t\tconsole.log(bodyFrame.bodies[i]);\r\n\t\t\t}\r\n\t\t}\r\n\t});\r\n\r\n\t//request body frames\r\n\tkinect.openBodyReader();\r\n\r\n\t//close the kinect after 5 seconds\r\n\tsetTimeout(function(){\r\n\t\tkinect.close();\r\n\t\tconsole.log(\"Kinect Closed\");\r\n\t}, 5000);\r\n}\r\n```\r\n","starsCount":2,"created":"2014-12-11T19:31:47.055Z","modified":"2016-10-30T10:43:51.112Z","lastPublisher":{"name":"wouterverweirder","email":"wouter.verweirder@gmail.com"},"owners":[{"name":"wouterverweirder","email":"wouter.verweirder@gmail.com"}],"other":{"_attachments":{},"_from":".","_id":"kinect2","_nodeVersion":"7.0.0","_npmOperationalInternal":{"host":"packages-18-east.internal.npmjs.com","tmp":"tmp/kinect2-0.1.5.tgz_1477824230492_0.009690823033452034"},"_npmUser":{"name":"wouterverweirder","email":"wouter.verweirder@gmail.com"},"_npmVersion":"4.0.1","_rev":"2-49140a6548267968e859aaad5607f2bc","_shasum":"bead8c7a76b1cf121ff6aeb3774d7dba31d8a832","author":{"name":"Wouter Verweirder"},"bugs":{"url":"https://github.com/wouterverweirder/kinect2/issues"},"directories":{},"dist-tags":{"latest":"0.1.5"},"dist":{"shasum":"bead8c7a76b1cf121ff6aeb3774d7dba31d8a832","tarball":"http://registry.npmjs.org/kinect2/-/kinect2-0.1.5.tgz"},"maintainers":[{"name":"wouterverweirder","email":"wouter.verweirder@gmail.com"}],"readmeFilename":"README.md","time":{"modified":"2016-10-30T10:43:51.112Z","created":"2014-12-11T19:31:47.055Z","0.0.7":"2014-12-11T19:31:47.055Z","0.0.8":"2014-12-11T19:32:54.507Z","0.0.9":"2015-07-24T16:53:17.748Z","0.0.10":"2015-07-25T13:35:15.105Z","0.0.11":"2015-07-25T13:38:07.937Z","0.0.12":"2015-07-25T18:57:09.766Z","0.0.13":"2015-07-26T19:12:43.37